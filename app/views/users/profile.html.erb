<h1>Dashboard</h1>
<div class="user_title"><h1> <%=@user.username%>'s Profile</h1>
	<% if user_signed_in? %>
		<% if current_user.username == @user.username %>
    	<%= link_to 'Edit Profile', edit_user_registration_path %>
   				<% @reservation.each do |r| %>
   				<div>
   					<%= r.start_date.to_s.to_date %> -
   					<%= r.end_date.to_s.to_date %>
   					<a href="../parkingspots/<%= r.parkingspot_id %>">Parkingspot show page</a>
   				<%#= link_to "Reservation Confirmation!", parkingspot_reservation_confirmation_path %>
   					<a href="../parkingspots/<%= r.parkingspot_id %>/reservations/<%= r.id %>">Reservation Confirmation</a>
   				</div>
   			<% end %>
    	<% end %>
    <%end %>
</div>



		<br>
<%= link_to "Rent parking spot form", new_parkingspot_path %>


	My Parking Spots
	<% @parkingspot.each do |p| %>
	<div>
		location <%= p.address %>
		<br>
		rate $<%= p.price %>
		<br>
		rented? <%= p.rented %>
		<br>
		name <%= p.listing_name %>
<hr>
</div>
	<% end %>

</div>